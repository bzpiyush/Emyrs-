// Check migration status from AzMoveDiagnostics
// Replace <TENANT_NAME> with the tenantName from the get-tenant-from-vm query

let tenantName = "<TENANT_NAME>"; // e.g., "5bf58680-bab3-4628-86f1-84b7d97ef0e6"

AzMoveDiagnostics
| where PreciseTimeStamp > ago(1d)
| where * contains tenantName
| order by PreciseTimeStamp desc
| project 
    PreciseTimeStamp,
    OperationName,
    Status,
    Message,
    DurationMs
| limit 100
